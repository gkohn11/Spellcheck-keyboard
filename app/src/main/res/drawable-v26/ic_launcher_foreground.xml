<!-- Since PNG files are already cropped, use smaller or no insets -->
<!-- Adjust these values: 0% = no zoom, negative = zoom in, positive = zoom out -->
<inset xmlns:android="http://schemas.android.com/apk/res/android"
    android:drawable="@mipmap/ic_launcher"
    android:insetLeft="0%"
    android:insetTop="0%"
    android:insetRight="0%"
    android:insetBottom="0%" />
